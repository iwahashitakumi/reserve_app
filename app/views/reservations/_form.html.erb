<%= form_with model: @room.reservations.new do |f| %>
<table>
   <tr>
     <th><%= f.label :start_date ,"チェックイン" %></th>
     <td><%= f.date_field :start_date,  placeholder: "イン" %></td>
   </tr>

   <tr>
     <th><%= f.label :start_end ,"チェックアウト" %></th>
     <td><%= f.date_field :end_date,  placeholder: "アウト" %></td>
   </tr>
   <tr>
     <th><%= f.label :people ,"人数" %></th>
     <td><%= f.text_field :people,  placeholder: "0" %></td>
   </tr>

   <%= f.submit "予約する"  %>

<% end %>
   